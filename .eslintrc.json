{
  "extends": "airbnb",
  "env": {
    "node": true,
    "browser": true,
    "es6": true
  },
  "plugins": [
    "react",
    "jsx-a11y"
  ],
  "settings": {
    "import/resolver": "webpack"
  },
  "parserOptions": {
    "ecmaFeatures": {
        "experimentalObjectRestSpread": true
    }
  },
  "rules": {
    "react/jsx-filename-extension": ["warn", { "extensions": [".js", ".jsx"] }],
    "react/require-extension": "off",
    "semi": ["error", "never"]
  }
}